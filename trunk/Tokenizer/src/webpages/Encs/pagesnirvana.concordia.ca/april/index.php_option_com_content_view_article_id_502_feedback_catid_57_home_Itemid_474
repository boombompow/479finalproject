<script language="javascript"><!--
function validateFeedback(formObj){
	if (emptyField(formObj.yourname))
  	alert("Please enter your name.");
  else if (emptyField(formObj.youremail))
  	alert("Please enter your email.");
  else if (validateEmail(formObj.youremail))
  	alert("You have entered an invalid email address.");
  else if (emptyField(formObj.comments))
  	alert("Please enter your comment.");
  
  else return true;
	
  return false;
}
function emptyField(textObj){
	if (textObj.value.length == 0) return true;
	for (var i=0; i<textObj.value.length; ++i) {
	    var ch = textObj.value.charAt(i);
	    if (ch != ' ' && ch != '\t') return false;
	}
	return true;
}
function validateEmail(textObj){
	var counter = 0;
  for (var i=0; i<textObj.value.length; ++i) {
		var ch = textObj.value.charAt(i);
  	if (ch == '@' || ch == '.'){ counter++; }
  	if (counter >=2 ){ return false; }
	}
  return true;
}
function isChecked(checkedbox) {
	var valid = false;
	if (checkedbox.checked) {
	    return false;
	}
	return true;
}
// --></script>
