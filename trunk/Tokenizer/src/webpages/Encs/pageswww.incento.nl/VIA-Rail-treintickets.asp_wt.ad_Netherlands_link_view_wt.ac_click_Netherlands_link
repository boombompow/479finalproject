<script language="JavaScript" type="text/javascript"> 
function funResizeHeight(){
	if (document.getElementById('Kolom_Content') != null && document.getElementById('Kolom_Ace') != null){
		var intKolomHeight 	= document.getElementById('Kolom_Content').offsetHeight;
		var intAceHeight 	= document.getElementById('Kolom_Ace').offsetHeight;
		if (intKolomHeight > intAceHeight){document.getElementById('Kolom_Ace').style.height=(intKolomHeight)+"px";}
		else{document.getElementById('Kolom_Content').style.height=(intAceHeight-30)+"px";}
	}
}
funResizeHeight();
</script>
