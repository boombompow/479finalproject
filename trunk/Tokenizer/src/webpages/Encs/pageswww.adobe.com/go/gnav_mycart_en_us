<script type="text/javascript">
	$(document).ready(function() {
		/*global jQuery,adobe*/
		jQuery.createNs("Adobe.Cart.ShoppingCart");

		Adobe.Cart.ShoppingCart.ShoppingCartItemEdit = {
			/*
				params:
					pid - Product key to generate configurator for
					container - DOM element to render configurator at
					priceLabel - DOM element to render price label at
					countryCode - Current country for the given locale
					planLabel - true to show the plan label, false otherwise
					marketSegment - Current market segment for the given locale
			 */	
			loadConfigurator: function(params, cartItem) {
			 	var planLabel = (params.planLabel ? 'Plan:' : '');
				var configurator = $(params.container).product_configurator({
					productKey : params.pid,
					country : params.countryCode || "US",
					marketSegment : params.marketSegment || "COM",
					domainURL : "http://store1.adobe.com/svcs",
					storeName : "",
					cartMode: true,
					overrideSingleSku: true,
					productModel: params.productModel,
					defaultValues: cartItem.getDefaultConfigurationValues(),
					localizedLabels : {
						DISTRIBUTION_LABEL			:'I want to buy:',
						DISTRIBUTION_PROMPT			:'Select',
						UPGRADE_LABEL 				:'I own:',
						UPGRADE_PROMPT 				:'Select Product',								
						FULFILLMENT_LABEL			:'Delivery:',
						FULFILLMENT_PROMPT	 		:'Choose a delivery',								
						LANGUAGE_LABEL 				:'Language:',
						LANGUAGE_PROMPT 			:'Choose a language',								
						PLATFORM_LABEL 				:'Platform:',
						PLATFORM_PROMPT 			:'Choose a platform',								
						SERVICE_COMMITMENT_LABEL 	: planLabel,
						SERVICE_COMMITMENT_PROMPT 	:'Choose a plan',
						TERM_TYPE_LABEL 			:'Subscription Type:',
						TERM_TYPE_PROMPT			:'Choose a subscription',
						VERSION_LABEL				:'I own:',
						VERSION_PROMPT				:'Select Product',							
						QUANTITY_LABEL				:'Quantity:'
					},
					width 				: 120,
					optionsWidth 		: 200,
					priceLabel			: $(params.priceLabel),
					maxToggleWidth		: 105
				});
				return configurator;
			}
		}
	});				
</script></div>

	
