    <script type="text/javascript">
        var pageCookie=getCookie('software+');
        
		var isEduMode=false;

		
        if (pageCookie!=null && pageCookie!="" && "/content/dotcom/en/products/catalog/software.html".search("_sl_id-contentfilter_sl") == -1)
          {
          	
          	var url = "/content/dotcom/en/products/catalog/software.html".replace("html", pageCookie);
		    url= Adobe.Product.Controller.CPod.EduController.addMarketSegmentParam(url);
            window.location=url;
          	
          } 
         
        
        function getCookie(c_name)
		{
			var i,x,y,ARRcookies=document.cookie.split(";");
			for (i=0;i<ARRcookies.length;i++)
			{
			  x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
			  y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
			  x=x.replace(/^\s+|\s+$/g,"");
			  if (x==c_name)
			    {
			    return unescape(y);
			    }
			}
		}  
    </script>
