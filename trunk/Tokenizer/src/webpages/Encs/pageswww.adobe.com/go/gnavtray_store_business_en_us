<script type="text/javascript">
// Initialize the ShoppingCartService
ShoppingCartService.init({
  url: '/integration/job/request/ShoppingCartService/EveryoneGroup/group/?SiteID=adbevlus',  
  siteID: 'adbevlus',
  locale: 'en_US',
  fieldsToBeReturned: {
    baseFields: ['displayName','productType'],
