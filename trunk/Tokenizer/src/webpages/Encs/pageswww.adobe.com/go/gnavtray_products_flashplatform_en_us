	function ProcessXMLFile(req) {
		// This block is necessary for IE because it doesn't create
		// an XML DOM (responseXML) when loading files off disk via file://.
	
		var doc = req.xhRequest.responseXML;
		if (!doc || !doc.firstChild) {
			doc = Spry.Utils.stringToXMLDoc(req.xhRequest.responseText);
			if (!doc || !doc.firstChild) {
				alert("Failed to get XML document DOM!");
				return;
			}
		}
	
		// Convert the XML DOM document to a JS object.
		var str = "";
		var xml = Spry.XML.documentToObject(doc);
		
		var postHeadline = new Array();
		var postURL = new Array();
		var postDate = new Array();
		var postDateYear = new Array();
		var postDateMonth = new Array();
		var postDateDay = new Array();
		var postBlogHeader = new Array();
		var postBlogDate = new Array();

		for (var bi=0; bi<=4; bi++) {
			postHeadline[bi] = xml.feed.entry[bi].title._value();
			postURL[bi] = xml.feed.entry[bi].content["@xml:base"];
			postDate[bi] = xml.feed.entry[bi].published._value();
			postDateYear[bi] = postDate[bi].slice(0, 4);
			postDateMonth[bi] = postDate[bi].slice(5, 7);
			postDateDay[bi] = postDate[bi].slice(8, 10);
			postDateMonth[bi] = changeMonth(postDateMonth[bi]);
			postDate[bi] = postDateDay[bi] + " " + postDateMonth[bi] + " " + postDateYear[bi];
			postBlogHeader[bi] = "<h5><a href=\"" + postURL[bi] + "\" target=\"_blank\">" + postHeadline[bi] + "</a></h5>";
			postBlogDate[bi] = "<p class=\"stamp-date\">" + postDate[bi] + "</p>";
		}
		
		postBlogMoreLink = "<p><a class=\"link-more\" href=\"http://blogs.adobe.com/flashplatform/\" target=\"_blank\">Read more</a></p>";
		
		$('blog_postings').insert( {'top':postBlogMoreLink} );

		for (var bi=4; bi>=0; bi--) {
			$('blog_postings').insert( {'top':postBlogDate[bi]} );
			$('blog_postings').insert( {'top':postBlogHeader[bi]} );
		}
	}
	
	function changeMonth(monthVal) {
		if (monthVal == "01") {
			return "Jan";
		}
		else if (monthVal == "02") {
			return "Feb";
		}
		else if (monthVal == "03") {
			return "Mar";
		}
		else if (monthVal == "04") {
			return "Apr";
		}
		else if (monthVal == "05") {
			return "May";
		}
		else if (monthVal == "06") {
			return "Jun";
		}
		else if (monthVal == "07") {
			return "Jul";
		}
		else if (monthVal == "08") {
			return "Aug";
		}
		else if (monthVal == "09") {
			return "Sep";
		}
		else if (monthVal == "10") {
			return "Oct";
		}
		else if (monthVal == "11") {
			return "Nov";
		}
		else if (monthVal == "12") {
			return "Dec";
		}
	}	
		
	window.onload=function LoadXMLFile() {
		var url = "/cfusion/ajax/index.cfm?whitelist=flashplatform&target=http://blogs.adobe.com/flashplatform/feed/atom";
		Spry.Utils.loadURL("GET", url, true, ProcessXMLFile);
	}
//-->
