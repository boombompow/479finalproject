<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/ubi/template/identity/lib/swfobject2.2/swfobject.js"></script>

<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/js/store_util.js"></script>
<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/js/store/ubiCartUtils.js"></script>
<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/ubi/template/identity/adobe/CartFascade.js"></script>
<script type="text/javascript" src="http://wwwimages.adobe.com/www.adobe.com/ubi/template/identity/adobe/URLParser.js"></script>
<script type="text/javascript">
	var cart = new adobe.CartFascade();
	cart.setRequiredFlashVersion("9.0.115");
	
	if(adobe.URLParser.locale != 'en_us')
		cart.setDefaultCountry(adobe.URLParser.locale);
	else
		cart.setDefaultCountry("");	
	
	if(cart.isOverlaySupported()) {	
		adobe.link("adobe/Pane/screen.css");
		var PromoIDReport = new Array();
		
		function handleCartButton(e, distMethod, store_type) {
			e.stop();
			
			var ProductName = this.title.gsub(/Buy|Upgrade\s/,'');
			var CategoryPath = this.id.gsub(/CartFull-|CartUpgrade-|CartSubscription-/,'').gsub('-','/');	
			cart.setCategoryPath("/"+CategoryPath);
			cart.setProductName(ProductName);
			
			
			var hrefString = this.search;
			var qs = hrefString.toQueryParams();
			
			if(qs['promoid']) {
				var within = ( PromoIDReport.indexOf(qs['promoid']) == -1 ) ? false : true; 
				var promoid = (!within) ? qs['promoid'] : "";
				if(!within) PromoIDReport.push(qs['promoid']);
				cart.setEmailTrackingId(promoid);			 
			}
			
			cart.setDistributionMethod(distMethod);
			cart.setStoreType(store_type);
			cart.openCartOverlay(e);
		}
		
		 Event.observe(window, "load", function() {
			$$("a[rel=cartupgrade]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"UPGRADE", "COM"));
			});
			
			$$("a[rel=cartfull]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"FULL", "COM"));
			});
			
			$$("a[rel=cartsubscription]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"SUB_NEW", "COM"));
			});
			
			$$("a[rel=EDUcartupgrade]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"UPGRADE", "EDU"));
			});
			
			$$("a[rel=EDUcartfull]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"FULL", "EDU"));
			});
			
			$$("a[rel=EDUcartsubscription]").each(function(element) {
				element.observe("click", handleCartButton.bindAsEventListener(element,"SUB_NEW", "EDU"));
			});
		});  
	} 
</script>


