// Data for Meg
var productCount = 0;
// Data for Meg
var omtrProductList = "";
var docid = "";
var urlMatch = /(https?:\/\/)([-\w\.]+)+(:\d+)?(\/([\w\/_\.]*))(index.html)?(\?\S+)?(\#\S+)?/;
var adobeMatch = /(https?:\/\/)([-\w\.]+)+(\.adobe\.com)/;
// Useful data
var pageUrl   = document.location.href;
var htmlTitle = document.getElementsByTagName("title");
var pageTitle = htmlTitle[0].innerHTML;
var pageTitleLowerCase = pageTitle.toLowerCase();
// Data for Recommendatios
var categoryId = "";
var mDate = "";

var metaElements = document.all ?
document.all.tags('meta') :
document.getElementsByTagName ?
document.getElementsByTagName ('meta') : new Array();

// normalize to remove the default page name, parameters and anchors
pageUrl = pageUrl.replace(/index.html/, "");
if (urlMatch.test(pageUrl)) {
	var matchArray = urlMatch.exec(pageUrl).slice();
	var newUrl = matchArray[1];
	if ( typeof(matchArray[2]) != 'undefined') {
		newUrl = newUrl + matchArray[2];
	}
	if (adobeMatch.test(newUrl)) {
		if ( typeof(matchArray[3]) != 'undefined') {
			newUrl = newUrl + matchArray[3];
		}
		if ( typeof(matchArray[4]) != 'undefined') {
			newUrl = newUrl + matchArray[4];
		}
		pageUrl = newUrl;
	}
}

// Bug # 2955756: Redirects for http://kb2.adobe.com/cps/865/cpsid_86551.html
