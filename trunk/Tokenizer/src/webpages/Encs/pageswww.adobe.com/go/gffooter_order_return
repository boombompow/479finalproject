	document.location = "http://kb2.adobe.com/cps/908/cpsid_90893.html";
}

for (var m = 0; m < metaElements.length; m++) {
	if (metaElements[m].name == "datemodified") {
		mDate = metaElements[m].content;
	}
}

for (var m = 0; m < metaElements.length; m++) {
	if (metaElements[m].name == "product") {
		productCount++;
		if (omtrProductList.length > 0) {
			omtrProductList = omtrProductList + ", ";
