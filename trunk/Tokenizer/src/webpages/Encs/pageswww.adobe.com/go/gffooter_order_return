<script src="http://kb2.adobe.com/cps/ssi/assets/jquery-1.5.1.min.js" type="text/javascript"></script>

<script type="text/javascript">
	var disabledDocIds = ["tn_15327","1008920", "kb405819"];
	// Just turn the Dynamic Nav off completely now.
	var isDisabled=true;
	var docId = location.pathname.substring( location.pathname.lastIndexOf('/')+1, location.pathname.lastIndexOf('.') );
	if (jQuery.inArray(docId,disabledDocIds)!=-1) {
		isDisabled=true;
	}
</script>

<script type="text/javascript">
	jQuery.noConflict();
	// Configuration for jQuery.query plugin
	jQuery.query={spaces:false};
</script>
<script src="http://kb2.adobe.com/cps/ssi/assets/jquery.query.js" type="text/javascript"></script>
<script type="text/javascript">
	var stage=(document.location.href.indexOf(".stage.") != -1) ? 'stage.' : '';
	var baseUrl='http://www.'+stage+'adobe.com';
	var navApiServer="api.community.adobe.com";
	var navApiUrl='http://' + navApiServer + '/similarity/api/1.0/suggestions/jsonp';
	//var morePageUrl='http://www.stage.adobe.com/support/morehelp/index.html';
	var tagPageUrl=baseUrl+'/support/tagsearch/index.html';
	var linksCount=5;
	var supportedProducts=[
		'flash',
		'flash lite',
		'photoshop'
	];
	var product=[];
	jQuery('meta[name="product"]').each(function(){
		var s=jQuery(this).attr('content');
		if (s && s!='') {
			product.push(s.toLowerCase());
		}
	});
	var isSupported=false;
	jQuery.each(product,function(){
		if (jQuery.inArray(this.toString(),supportedProducts)!=-1) {
			isSupported=true;
			return false;
		}
	});
	var locale='en_US';
	var agt=navigator.userAgent.toLowerCase();
	var use_chc_behavior = false;

	if (agt.indexOf("adobe help viewer 2") != -1 || agt.indexOf("community help client") != -1) {
		use_chc_behavior = true;
	}

	function decodeStr(str) {
		//return str.toString().replace(/&amp;/g,"&");
		return jQuery('<textarea />').html(str.toString()).val();
	}
	function showLinks(id,result,count) {
		var i=0;
		//jQuery('#'+id).empty();
		jQuery.each(result.result.suggestions[0],function(){
			if (i>=count) {
				return false;
			}
			//var title='<a href="http://kb2.adobe.com/cps/912/'+this.url+'">'+decodeStr(this.title)+'</a>';
            var title='<a onclick="\'javascript:trackIt(" "nodetry", "", this.href )\' href="http://kb2.adobe.com/cps/912/'+this.url+'">'+decodeStr(this.title)+'</a>';
			if (this.url.toLowerCase().indexOf('adobe.com')==-1) {
				title+='&nbsp;<span class="CHIcon"><img src="http://kb2.adobe.com/cps/ssi/assets/chicon.png" /></span>'
			}
			jQuery('<li/>').html(title).appendTo('#'+id);
			i++;
		});
		return i;
	}
	function getTagLink(tag) {
		if (product.length==1) {
			return tagPageUrl+jQuery.query.empty().set('products',product[0].toLowerCase()).set('tags',tag.toString()).set('locale',locale).set('pageSize',20).toString();
		}
		return tagPageUrl+jQuery.query.empty().set('tags',tag.toString()).set('locale',locale).set('pageSize',20).toString();
	}
	function showTags(result) {
		//var tags=(result.result.relatedtags)?(result.result.relatedtags):(result.result.tags);
		var tags=result.result.page.tags;
		if (tags) {
			if (tags.length>0) {
				jQuery('#TagsBox').show();
			}
			var product = result.result.page.products;
			jQuery.each(tags,function(){
				var tag=decodeStr(this);
				//jQuery('<span/>').addClass('tag').html('<a href="http://kb2.adobe.com/cps/912/'+getTagLink(tag)+'">'+tag+'</a>').appendTo('#TagsBox');
				jQuery('<span/>').addClass('tag').html('<a onclick="\'javascript:trackIt(" "nodetag", "'+tag+'", this.href )\' href="http://kb2.adobe.com/cps/912/'+getTagLink(tag)+'">'+tag+'</a>').appendTo('#TagsBox');
				 
			});
		}
	}
	function loadApi(queryObj) {
		document.write('<script type="text\/javascript" src="http://kb2.adobe.com/cps/912/'+navApiUrl+queryObj.toString()+'"><\/script>');
	}
	function showMoreLinks(result) {
		if (showLinks('MoreLinks',result,linksCount)>0) {
			jQuery('#MoreLinksBox').show();
		}
	}
	function showOtherLinks(result) {
		if (showLinks('OtherLinks',result,linksCount)>0) {
			jQuery('#OtherLinksBox').show();
		}
	}
	function loadAllLinks(){ 
		if (!use_chc_behavior) {
			jQuery('#search-form').show();
		}
		if (isSupported && ! isDisabled){
			var queryObj=jQuery.query.empty().set('pageSize',linksCount).set('locale',locale).set('page',location.href.toString().replace(".stage.",".").replace(".dev.",".").replace("?debug=true","")).set('jsonCallback','showMoreLinks');
			loadApi(queryObj);

			/*
			if (product.length>0) {
				queryObj=queryObj.set('products',product.toString());
			}
			queryObj=queryObj.set('jsonCallback','showOtherLinks');
			loadApi(queryObj);
			*/

			queryObj=jQuery.query.empty().set('page',location.href.toString().replace(".stage.",".").replace(".dev.",".").replace("?debug=true","")).set('locale',locale).set('maxTagsNumber','10').set('jsonCallback','showTags');
			loadApi(queryObj);

		}
	}
	

    function trackIt(section, tag, url){
        // set OMT variables
        s=s_gi(s_accountName); 
        if (section == "NodeTag"){
			url = url.replace('http://www.adobe.com/support/','');
			s.prop23 = s_prop23 = "KB: " + section + ": " +  tag + ": " +url;		
		}else{
			s.prop23 = s_prop23 = "KB: " + section + ": " + url;
        }
		s.t();
        
        // debugging mode
        if ( top.location.href.indexOf("debug=true") > -1 ){
            alert ( s.prop23 );
        }
    }
	
	
</script>

<!-- Search box -->
<div id="search-form">
    <script type="text/javascript">
		if (document.location.href.indexOf("kb2") > 0) {
			document.write('<form method="get" name="searchform" action="http://www.adobe.com/cfusion/search/index.cfm" id="searchform">');
			document.write('<input type="hidden" name="loc" value="'+locale+'" />');
		} else {
			document.write('<form method="get" name="searchform" action="https://internalapps.adobe.com/cfusion/intranet/support/search/index.cfm" id="searchform">');
			document.write('<input type="hidden" name="loc" value="'+locale.substring(0,2)+'" />');
		}
	</script>
    <p>Adobe Community Help</p>
    <div class="SearchBox">
        <div class="SearchInputBox">
            <input type="text" class="PocketSearch" id="search-support" name="term" value='Search' onclick="if(this.value=='Search')value='';" onblur="if(this.value=='') this.value='Search';" />
        </div>
    </div>
    <input type="hidden" name="cat" value="support" />
    <script type="text/javascript">
		document.write('</form>');
	</script>
</div>
