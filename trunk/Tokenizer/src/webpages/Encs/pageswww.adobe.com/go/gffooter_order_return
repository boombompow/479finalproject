<h1>Help acquiring your product | Orders, returns</h1><div id="badgeAnchor"></div>

<div id="badgeContainer">
	<noscript>
		<span style="color:#990000;font-style:italic;">To view/add comments, you must enable JavaScript in your browser.</span>
	</noscript>
</div>

<script type="text/javascript" src="http://community.adobe.com/help/badge/ionSupport.js">
</script>
<div id="tntRatingEvent"></div>
<script type="text/javascript">
	function kbRatingCallback(ratingJsonObject) {
		var ratingValue = ratingJsonObject.ion.thumbs[0].content;
		// Substitute rating values from Ion with those preferred for the Recommendations pilot
		if (ratingValue.indexOf("up") == 0) {
			ratingValue = "Yes";
		}
		else {
			ratingValue = "No";
		}
		var s=s_gi(s_accountName);
		s.prop23 = s_prop23 = ratingValue + ":" + window.location.toString().replace(/\?(.*)/, "");
		s.t();
		if (document.location.href.indexOf("debug=true") != -1) {
			alert("SiteCatalyst account: " + s_accountName);
			alert("SiteCatalyst account object: " + s);
			alert("Sent this value from s.prop23 to SiteCatalyst using s.t(): " + s.prop23);
		}
		if ((recDataFlag || recDisplayFlag) && pageUrl.indexOf("preview") == -1) {
			mboxDefine('tntRatingEvent','LRRec_enus_Support_Multiple_RelatedPages_Ratings');
			var directrec = "No";
			// Was this page loaded by following a Recommendations link?
			if (document.location.href.indexOf("rec=true") != -1) {
				directrec = "Yes";
			}
			// Update Omniture with the current rating
			mboxUpdate('LRRec_enus_Support_Multiple_RelatedPages_Ratings',
				// for displayData
				// 'entityId=' + pageUrl ,
				// for descriptionData
				'entity.id=' + pageUrl + '_' + ratingValue ,
				'entity.name=' + pageTitle ,
				'entity.pageUrl=' + pageUrl ,
				'entity.categoryId=' + categoryId , 
				'entity.rating=' + ratingValue ,
				'entity.subCatalog=LearningResources' , 
				'entity.directrec=' + directrec ,
				'entity.mDate=' + mDate
				);
		}
	}

	var socialSkyDataLimit = 4096;
	if (ion_product_list.length > socialSkyDataLimit) {
		ion_product_list = ion_product_list.substring(0,ion_product_list.lastIndexOf("",socialSkyDataLimit));
	}
	var patrn = (/(.*)-(.*)/);
	if(patrn.test(ion_locale)) {
		ion_locale = ion_locale.gsub(/(.*)-(.*)/,function(match){
			return match[1] + "_" + match[2].toUpperCase();
		});
	}						  
	ionReadyLoad(function(){
		var badge = new ionSupportBadge();
		badge.url = window.location.toString().replace(/\?(.*)/, "");
		badge.locale = ion_locale;
		badge.commenting = true;
		badge.rating = true;
		badge.ratingType = "thumbs";
		badge.container = document.getElementById("badgeContainer");
		badge.anchor = document.getElementById("badgeAnchor");
		badge.siteArea = (document.location.href.indexOf("kb2") > 0) ? "support" : "internal";
		badge.timeOutDelay = 30000;
		badge.productLabel = ion_product_list.strip().split(" ");
		if (typeof kbRatingCallback == "function") {
			badge.ratingCallbackQueue = kbRatingCallback;
		}
		badge.load();
	});
</script>
