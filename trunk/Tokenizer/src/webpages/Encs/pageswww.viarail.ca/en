<script>
	
	function diapoCenter() { //Create a function that center the Slideshow
		var windowWidth = $(window).width(); //Get the width of the window
		var windowWithoutDiapo = windowWidth - 720; 
		var positionLeft = windowWithoutDiapo/2;	
		$("#video_container").css({ 'left' : positionLeft}); //Set exact width of row in pixels instead of using % - Prevents cross-browser bugs that appear in certain view port resolutions.
	}
	
	$(document).ready(function(e) {
		
		var divVideo = '<div id="wrapper_video_ie7"><div id="transp_video_container"></div><div id="video_container"><div class="close_video"><img src="http://www.viarail.ca/sites/all/files/media/images/landing-pages/2011/Q1/img/close_video.jpg" width="13" height="14" /></div><object classid="clsid:d27cdb6e-ae6d-11cf-96b8-444553540000" codebase="http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,40,0" height="412" width="620"><param name="quality" value="high" /><param name="wmode" value="opaque" /><param name="movie" value="/sites/all/files/media/images/landing-pages/2011/Q1/video/via_EN.swf" /><embed height="412" pluginspage="http://www.macromedia.com/go/getflashplayer" quality="high" src="http://www.viarail.ca/sites/all/files/media/images/landing-pages/2011/Q1/video/via_EN.swf" type="application/x-shockwave-flash" width="620" wmode="opaque"></embed></object></div></div>';
			
		$('.bloc_video_left img').click(function(e){
			$('#layout-rightside').after(divVideo);
			$('#transp_video_container').fadeIn('normal');
			$('#video_container').fadeIn('slow', callback);
			diapoCenter();
			$(window).resize(function () { //Each time the window is adjusted/resized, execute the function
				diapoCenter();
			});
		});
		
		function callback(){
			$('#transp_video_container').click(function(){
				$('#video_container').fadeOut();
				$('#transp_video_container').fadeOut();
			});
			
			$('.close_video img').click(function(){
				$('#video_container').fadeOut();
				$('#transp_video_container').fadeOut();
			});
		}
	});
	
</script> </div>
