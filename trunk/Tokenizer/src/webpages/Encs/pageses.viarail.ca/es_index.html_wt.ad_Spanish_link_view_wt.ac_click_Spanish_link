<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript"><!--
//selectDate();

 var HaveCookie;
function GetCookie (name) {
	var arg = name + "=";
	var alen = arg.length;
	var clen = document.cookie.length;
	var i = 0;
	while (i < clen) {
	var j = i + alen;
	if (document.cookie.substring(i, j) == arg)
	return getCookieVal (j);
	i = document.cookie.indexOf(" ", i) + 1;
	if (i == 0) break;
	}
	return null;
}


function SetCookie (name, value) {
	var argv = SetCookie.arguments;
	var argc = SetCookie.arguments.length;
	var expires = (argc > 2) ? argv[2] : null;
	var path = (argc > 3) ? argv[3] : null;
	var domain = (argc > 4) ? argv[4] : null;
	var secure = (argc > 5) ? argv[5] : false;
	document.cookie = name + "=" + escape (value) +
	((expires == null) ? "" : ("; expires=" + expires.toGMTString())) +
	((path == null) ? "" : ("; path=" + path)) +
	((domain == null) ? "" : ("; domain=" + domain)) +
	((secure == true) ? "; secure" : "");
}


	SetCookie(HaveCookie,true);
	function testcookie(){
		if (GetCookie(HaveCookie) != "true"){
			alert ("Tome nota de que su soporte lógico o programa de navegación ha de estar configurado para aceptar  testigos  (o \"cookies\") para que su itinerario y la fecha de salida sean transmitidos a reserVIA.");
			return false;
		}
		else {
		return true;}
	}	

//--></SCRIPT><!-- <A HREF="/reservation" TARGET="_top"><IMG SRC="/images/en_bout_tarif_rese.gif" WIDTH="196" HEIGHT="26" ALT="" BORDER="0" VSPACE="5" HSPACE="5"></A><A HREF="/tickets" TARGET="_top"><IMG SRC="/images/en_bout_hora.gif" WIDTH="115" HEIGHT="26" ALT="" BORDER="0" VSPACE="5" HSPACE="5"></A> -->

<SCRIPT LANGUAGE="JavaScript" TYPE="text/javascript"><!--
function putdate(xssiJour,xssiMois){
//document.f.departureDay.selectedIndex = parseInt(xssiJour);
//document.f.departureMonth.selectedIndex = parseInt(xssiMois);
document.frmSearch_RV.VDL2_cmbDaysFrom.options[parseInt(xssiJour)].selected = true;
document.frmSearch_RV.VDL2_cmbMonthsFrom.options[parseInt(xssiMois)].selected = true;
}
d = new Date();
var xssiJour = "0";
var xssiMois = "0";
xssiJour = " 1"; 
xssiMois = "12";
if (xssiJour.indexOf("config") != -1) {xssiJour = (d.getDate())}
if (xssiMois.indexOf("config") != -1) {xssiMois = (d.getMonth()+1)}
putdate(xssiJour,xssiMois);
