	<TD BGCOLOR="#FFCC00" VALIGN="top" CLASS="CelTexteviapref"><BR>
		<!-- ================ <Accès_des_membres> ================= -->
		<script language="javascript" type="text/javascript"><!--// 
		//Affichage du formulaire d'identification
		GetVIACookieValue (GetCookie ("sessionkey"));
			if (oCurrentCookie.sessionkey == 'na' || oCurrentCookie.sessionkey == 'deleted' ){
				document.write ('<form action=\"/cgi-bin/Identification\" name=\"idlogin\" method=\"POST\" onsubmit=\"return Soumettre(this);\">');
				document.write ('<input type=\"hidden\" name=\"Commande\" value=\"email\" />');
				document.write ('<input type=\"hidden\" name=\"language\" value=\"es\" />');
				document.write ('<input type=\"hidden\" name=\"category\" value=\"accu\" />');
				document.write ('<input type=\"hidden\" name=\"debug\" value=\"reime\" />');
				document.write ('<input type=\"hidden\" name=\"pagedyn\" value=\"gestion_profil\" />');
				document.write ('<b>Inscríbase a nuestras comunidades virtuales!</B><BR>Reciba nuestros boletines de información o actualice su fichero.<br>');
				document.write ('<input type=\"text\" name=\"email\" value=\"nombre@vuestroservidor.com\" onclick=\"if(this.value==\'nombre@vuestroservidor.com\')this.value=\'\'\" onblur=\"if(this.value==\'\')this.value=\'nombre@vuestroservidor.com\'\" style=\"width:159px\"> <br> ');
				document.write ('<input type=\"submit\" name=\"submit\" class=\"sub\" value=\"Enviar\" onmouseover=\"modifon(this)\" onmouseout=\"modifoff(this)\" />');
				document.write ('</form>');
			}
			else {
				document.write('<b style="font-size : 12px ">Hola ' + oCurrentCookie.prenom + ' ' + oCurrentCookie.nom + "</b><br/>" );
				document.write('<a href="/cgi-bin/GestionProfil?Commande=select&category=accu&language=es&pk_profil=' + oCurrentCookie.pkprofil + '&sessionkey=' + oCurrentCookie.sessionkey +'">Actualice su expediente</a><br/>');
				document.write('<a href="javascript:DeleteCookie(\'sessionkey\');location.reload([true])">Cerrar sesión</a><br/><br/>');
			}
			
			function validerTexte (Objet, txtStr) {
				alert (txtStr)
					Objet.focus();
				return;
			}
			function testerBoite(f) {
				if (f.email.value == "" || f.email.value == "nombre@vuestroservidor.com") {
					validerTexte(f.email, "Le rogamos que escriba su dirección de correo electrónico");
					return (false);
				}
				if (f.email.value != "") {
					if(f.email.value.indexOf('@', 0) == -1 || f.email.value.indexOf('.',0) == -1 || f.email.value.indexOf(' ',0) != -1 || f.email.value.indexOf(',',0) != -1 )
						{
						alert('\nLa dirección de correo electrónico indicada no es exacta. Tiene que ser del siguiente tenor:\nnombre@vuestroservidor.com.\n');
						f.email.focus();
						return (false);
						}
				}
			return(true);
			}
			function Soumettre (form)  {
				if (!testerBoite(form)) return(false);
				return(true);
			}
			//--></script>
		<!-- ================ </Accès_des_membres> ================= -->
	
</TD>
