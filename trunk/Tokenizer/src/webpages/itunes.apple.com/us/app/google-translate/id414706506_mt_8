<script>
    if (!window.its) window.its = {};
    its.pageLoadStartTime = new Date().getTime();
    its.isDefined = function itsIsDefined(anObject) {
        return typeof(anObject) != 'undefined';
    }
	its.isDefinedNonNull = function itsIsDefinedNonNull(anObject) {
	  return (its.isDefined(anObject) && (anObject != null));
	};
</script>















	
	
	
	
	
	
	<script>
	     window._itsOnErrorCallbackRobust = function windowPrivateItsOnErrorCallbackRobust(message, serverDataDict) {
            try {  
                if (window.ITSLogger && ITSLogger.isLoaded) {
                    ITSLogger.logger(window.EXCEPTION_LOGGER_NAME).error(message, true, its.property("ITSLogger.FractionOfRequestsToReport.ITSExceptionHandler"), serverDataDict);
                } else if (window.ITSLogger) {
                    var fractionOfRequestsToReport = its.property ? its.property("ITSLogger.FractionOfRequestsToReport.ITSExceptionHandler") : null;
                    
                    ITSLogger.queueMessage(window.EXCEPTION_LOGGER_NAME, ITSLogger.ERROR, message, true, fractionOfRequestsToReport, serverDataDict);
                } else {
                    window._itsOnErrorCallbackFailsafe(message, serverDataDict);
                }
            } catch (anException) {
                window._itsOnErrorCallbackFailsafe("Exception in exception logging: "+anException.toString()+", url:"+anException.sourceURL+", lineNumber:"+anException.line);
            }
	    }	
	</script>
	
	<script>
	    window.EXCEPTION_LOGGER_NAME = "ITSExceptionHandler"; // IMPORTANT: If you change this name, be sure to change the ITSLogger.FractionOfRequestsToReport.ITSExceptionHandler that may go along with it!
	    window._itsComposeErrorMessage = function windowPrivateItsComposeErrorMessage(message, url, lineNumber, loggerName) {
            var loggerNameString = "";
            var urlString = "unknown (might be html-embedded script?)";
            var lineNumberString = "unknown";
	        
            if (its.isDefinedNonNull(url) && (url != "") && (url != "undefined")) urlString = url;
            if (its.isDefinedNonNull(lineNumber) && (lineNumber != "")) lineNumberString = lineNumber;
            if (loggerName) loggerNameString = loggerName+":";
	        var returnMessage = loggerNameString+"{message:\""+message+"\", url:\""+urlString+"\", lineNumber:\""+lineNumberString+"\"}";
	        
	        return returnMessage;
	    }
	    window._itsOnErrorCallback = function windowPrivateItsOnErrorCallback(message, url, lineNumber) {
	        // <rdar://problem/10407997> Improve ITSExceptionHandler's exception handling
            if (message && (message instanceof Event)) {
                var s = message.target;
                if (s) {
                    message = 'Error attempting to load a script: id='+s.id+' src='+s.src+' type='+s.type+', contents:'+s.innerHTML;
                }
            }
            var serverDataDict = {exceptionMessage:message, exceptionUrl:url, exceptionLineNumber:lineNumber};
            var fullMessage = window._attemptedDelayedShowEarlyExceptionComposedMessage; // Are we coming back here after a defer?
            if (fullMessage) {
                serverDataDict = window._attemptedDelayedShowEarlyExceptionServerDataDict;
            } else {
                fullMessage = window._itsComposeErrorMessage(message, url, lineNumber, window.EXCEPTION_LOGGER_NAME);
            }
	        if (window._itsOnErrorCallbackRobust) {	            
	            window._itsOnErrorCallbackRobust(fullMessage, serverDataDict);
	        } else {
                window._itsOnErrorCallbackFailsafe(fullMessage, serverDataDict);
	        }
	        window._tryShowingErrorIndicator(fullMessage)
	    }
	    window._tryShowingErrorIndicator = function windowPrivateTryShowingErrorIndicator(message) {
	        var NUM_RETRIES = 10;
            if (window._attemptedShowErrorIndicator == null) window._attemptedShowErrorIndicator = 0;

	        if (window._attemptedShowErrorIndicator < NUM_RETRIES) {
	            if (window.ITSLogger && ITSLogger.showErrorIndicator) {
	                window._attemptedShowErrorIndicator = NUM_RETRIES + 1; 
	                ITSLogger.showErrorIndicator(message);
	            } else if (window._attemptedShowErrorIndicator < NUM_RETRIES) {
	                // I've seen this take over 3 seconds!
	                window._attemptedShowErrorIndicator++;
	                window.setTimeout(function anonymousTryShowingErrorIndicator() {window._tryShowingErrorIndicator(message);}, 1000);
		        }
		    }
	    }
	    window._itsOnErrorCallbackFailsafe = function windowPrivateItsOnErrorCallbackFailsafe(composedMessage, serverDataDict) {
            // We only get here if this error happens *very* *very* early on... before we've parsed any support code or even set up our basic message queue.
            // So, before we completely bail with our failsafe, let's see if we can get the message out after a delay...
	        if (window._attemptedDelayedShowEarlyExceptionComposedMessage == null) {
                window._attemptedDelayedShowEarlyExceptionComposedMessage = composedMessage;
                window._attemptedDelayedShowEarlyExceptionServerDataDict = serverDataDict;
                window.setTimeout(window._itsOnErrorCallback, 1000);
	        } else {
                // We already tried deferring once... we now have no alternative but to simply spit the messageData to the console.
		        var  loggingMethod = null;
		        if (its.isDefined(iTunes) && iTunes.loggingEnabled && iTunes.log) {
		            loggingMethod = function(aMessage) {iTunes.log(aMessage);};
		        } else if (console && console.log) {
		            loggingMethod = function(aMessage) {console.log(aMessage);};
		        }
		        if (loggingMethod && composedMessage) {
		            loggingMethod(composedMessage);
		        }
		    }
	    }
	    window.onerror = window._itsOnErrorCallback;
	</script>

<!--     THIS CODE IS GOOD FOR TESTING THIS FEATURE ON ALL PLATRFORMS. IT FAKES IT ON iOS<5 AND HAS REAL ERRO ELSEWHERE -->
