<!-- [code comments are here to avoid spitting out JS comments to client in production] -->
<!-- ITSLogger (its-logger.js) INITIALIZATION (to be able to queue log messages before the logger has been loaded) -->
<!-- Used during ITSLogger's real initialization to copy this minimal class definition. We can't assign earlyLoadITSLogger in its-logger.js because -->
<!-- the interpreter will create the ITSLogger function (class) assignment before we get a chance to stash away that variable. -->
<!-- NOTE: All of these will get overwritten when we load the real its-logger.js code, but -->
<!-- this is defined here since we haven't included ITSLogger yet, but we want to start queueing up messages... -->
<script>
	if (!window.ITSLogger) {
	    window.ITSLogger = {};
	    window.earlyLoadITSLogger = window.ITSLogger;
	    ITSLogger.DEBUG = 1;
	    ITSLogger.INFO = 2;
	    ITSLogger.WARN = 3;
	    ITSLogger.ERROR = 4;
        ITSLogger.STATS = 5; // (pseudo-level that logs to the statsServer (Hubble) and logs to the console at "ITSLogger.INFO" level)
    }
    ITSLogger.timeAtLoad = new Date().getTime();
    
    ITSLogger.queueMessage = function ITSLoggerQueueMessage(loggerName, loggingThreshold, message, mirrorToServer, fractionOfRequestsToReport, serverDataDict) {
        if (arguments.length >= 3) {	        
            if (!ITSLogger._queuedMessages) ITSLogger._queuedMessages = [];
            ITSLogger._queuedMessages.push({
                loggerName: loggerName,
                loggingThreshold: loggingThreshold,
                message: message,
                serverDataDict: serverDataDict,
                mirrorToServer: mirrorToServer,
                fractionOfRequestsToReport: fractionOfRequestsToReport
            });
        }
    };
    ITSLogger.possiblyWaitForQueueMessages = function ITSLoggerLookForQueueMessages() {
        var timerInterval = its.property && its.property("itsLoggerQueueProcessingInterval");
        timerInterval = timerInterval ? timerInterval : 100;
	    if (ITSLogger._queuedMessages && ITSLogger._queuedMessages.length && timerInterval > 0) { 
            ITSLogger._delayedProcessQueuedMessages(timerInterval);
	    }
    }
    ITSLogger._processQueuedMessages = function ITSLoggerProcessQueuedMessages() {
        ITSLogger.possiblyWaitForQueueMessages();
    }
	ITSLogger._delayedProcessQueuedMessages = function itsLoggerDelayedProcessQueuedMessages(delay) {
	    window.setTimeout(ITSLogger._processQueuedMessages, delay);
	}
</script>


