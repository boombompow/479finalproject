<script language="javascript" type="text/javascript">
// Add contact_form_enabled css class to all forms on the page.
var reqForm = document.getElementsByTagName('form');
for(var j = 0; j < reqForm.length; j++) {
reqForm[j].className += " contact_form_enabled";
// Enable fields when javascript is enabled for all forms on page.
for(var i = 0; i < reqForm[j].elements.length; i++) {
if (reqForm[j].elements[i].className.indexOf('stay_disabled') < 1) {
reqForm[j].elements[i].disabled = false;
}
}
}
</script>
<script id="form-default-js" language="javascript" type="text/javascript">
// Autoselect the option in OS dropdown with value "win64"
// Note: This is done here because this can't be grabbed from
// the user agent string
if(window.navigator.platform == "Win64"
&& document.getElementById("winvista64")) {
document.getElementById("winvista64").selected = "selected";
}
/**
* Grab all fields in a fieldset, delete them from the fieldset,
* shuffle them up, and add them back onto the fieldset.
*
* @param {string} fieldSetId the id of the fieldset
* @param {boolean} hasOtherField Is there an Other field at the bottom?
*/
function randomFields(fieldSetId, hasOtherField) {
if(hasOtherField == null) {
hasOtherField = false;
}
var fieldset = document.getElementById(fieldSetId);
var allFields = fieldset.getElementsByTagName("div");
var fields = [];
var otherField = null;
// Remove fields from fieldset
if(hasOtherField) {
lastField = allFields[allFields.length-1];
otherField = lastField;
lastField.parentNode.removeChild(lastField);
}
for (var i = allFields.length-1; i >= 0; i--) {
var currField = allFields[i];
fields.push(currField);
currField.parentNode.removeChild(currField);
}
// Randomize the order of the fields
shuffleArray(fields);
// Attach the fields back onto the fieldset
while(currField = fields.pop()) {
fieldset.appendChild(currField);
}
if(hasOtherField) {
fieldset.appendChild(otherField);
}
}
/**
* Change the order of all the elements in an array
* to a random order
*/
function shuffleArray(arr) {
for (var i = 0; i < arr.length; i++) {
var elementsOrder = parseInt(Math.random() * arr.length);
var obj = arr[elementsOrder];
arr[elementsOrder] = arr[i];
arr[i] = obj;
}
}
</script>
