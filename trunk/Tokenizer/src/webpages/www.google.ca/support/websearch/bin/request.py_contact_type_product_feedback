window.formValidation_product_feedback= function(form,target) {
init(form,target);
var temp1;
var temp2;
validate('radio',
form.elements['extra.survey.overall_satisfaction'],
'Please tell us how satisfied you are with the product.');
validate("checkboxes",
"extra.survey.which_tasks_area",
"Please tell us which of the specified tasks you have tried to accomplish with the product.",
"1",
"100");
validate("maxlength",
form.elements['extra.survey.additional_search_feedback'],
"",
"");
var validated = checkErrors(form,target);
return validated;
};
</script>
