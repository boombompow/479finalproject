function selectSubCategory(id, number){
var selects = document.getElementById(id + "_options").getElementsByTagName('select');
for (var i=0; i<selects.length; i++) {
selects[i].style.display = "none";
selects[i].selectedIndex = 0;
}
var catForm = document.request;
catForm['extra.survey.cat' + number].value =
catForm[id].value;
showLayer(catForm[id].options[document.request[id].selectedIndex].value);
}
function showTextBox() {
var suggestionsEl = document.getElementById(
"extra.survey.cat1");
if (suggestionsEl && suggestionsEl.value != "") {
var disp = "";
} else {
var disp = "none";
}
document.getElementById("categorizer_openbox_div").style.display = disp;
}
